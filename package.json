{
  "name": "tauri-solidjs-template",
  "version": "0.1.0",
  "description": "DESCRIPTION",
  "author": {
    "name": "AUTHOR"
  },
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "tauri": "tauri",
    "lint": "pnpm run lint:js && pnpm run lint:rust",
    "lint:js": "eslint .",
    "lint:rust": "cd src-tauri && cargo clippy",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{json,html,css,md,yml,yaml}": [
      "prettier --write"
    ],
    "src-tauri/**/*.rs": [
      "cargo fmt --manifest-path src-tauri/Cargo.toml --",
      "cargo clippy --manifest-path src-tauri/Cargo.toml --fix --allow-dirty --allow-staged --"
    ]
  },
  "license": "MIT",
  "dependencies": {
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-opener": "^2",
    "solid-js": "^1.9.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@tauri-apps/cli": "^2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "lint-staged": "^16.2.7",
    "prettier": "3.8.1",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.54.0",
    "vite": "^6.0.3",
    "vite-plugin-solid": "^2.11.0"
  }
}
